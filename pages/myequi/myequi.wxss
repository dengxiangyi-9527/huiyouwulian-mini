  width: 34rpx;
}
.signal_menu image{
  width: 34rpx;
  height: 30rpx;
}
.signal_menu_box{
  position: absolute;
  width: 315rpx;
  height: 495rpx;
  background: #fff;
  border-radius: 20rpx;
  bottom: -500rpx;
  right: 5rpx;
  z-index: 3;
  transition: all 1s;
}
.signal_menu_item{
  padding: 20rpx 0 30rpx 40rpx;
