  border-bottom: 1rpx solid #e6e6e6;
}
.signal_menu_item .title{
  color: #7a7a7a;
}
.signal_menu_item .content{
  color: #0f0f0f;
  margin: 10rpx 0;
  border-bottom: 1rpx solid #f4f4f4;
}
.signal_menu_item .content .content_item{
  height: 36rpx;
  line-height: 36rpx;
}
.have-collect{
