/* header */
.header{
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  justify-content: space-around;
  /* background-color: #016299; */
  padding: 0 15rpx;
  text-align: center;
  position: relative;
}
.signal_pic{
  width: 28rpx;
}
.signal_pic image{
  width: 28rpx;
  height: 25rpx;
}
